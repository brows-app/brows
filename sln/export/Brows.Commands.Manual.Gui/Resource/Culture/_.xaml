                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
  <CompositeCollection x:Key="Manual_TextCollection">
    <TextBlock>
            <Run Text="Brows" FontWeight="Bold"/>
            <Run Text="is a file manager for power users."/>
    </TextBlock>
    <TextBlock>
            <Run Text="Control+N" Style="{DynamicResource CommandContextData_KeyboardKey}"/>
            <Run Text="opens a new panel."/>
    </TextBlock>
    <TextBlock>
            <Run Text="Tab" Style="{DynamicResource CommandContextData_KeyboardKey}"/>
            <Run Text="cycles through panels."/>
    </TextBlock>
    <TextBlock>
            <Run Text="Control+L" Style="{DynamicResource CommandContextData_KeyboardKey}"/>
            <Run Text="launches the command palette. From there, type commands or paths to navigate the file system."/>
    </TextBlock>
