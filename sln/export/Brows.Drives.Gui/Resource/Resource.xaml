<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:brows="http://schemas.brows.app/xaml"
                    xmlns:local="clr-namespace:Brows">
    <DataTemplate x:Key="DriveEntry_Name">
        <brows:EntryNameGrid Icon="{Binding Item[Icon]}" Text="{Binding Item[Name]}"/>
    </DataTemplate>
    <DataTemplate x:Key="PanelProvider_DriveProvider">
        <local:DriveProviderControl/>
    </DataTemplate>
</ResourceDictionary>
