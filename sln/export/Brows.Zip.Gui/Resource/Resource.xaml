<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:local="clr-namespace:Brows"
                    xmlns:brows="http://schemas.brows.app/xaml">
    <DataTemplate x:Key="ZipEntry_RelativeName">
        <brows:EntryNameGrid Icon="{Binding Item[Icon]}" Text="{Binding Item[RelativeName]}"/>
    </DataTemplate>

    <DataTemplate x:Key="PanelProvider_ZipProvider">
        <local:ZipProviderControl/>
    </DataTemplate>
</ResourceDictionary>
